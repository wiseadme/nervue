{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "allowJs": true,
    "incremental": true,
    "noEmit": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noEmitOnError": true,
    "strictNullChecks": true,
    "strictFunctionTypes": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": "./",
    "declarationDir": "dist/types",
    "outDir": "./dist",
    "composite": true,
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "scripthost",
      "webworker"
    ],
    "plugins": [
      {
        "afterDeclarations": true
      }
    ],
    "typeRoots": [],
    "types": [
      "@vue/compiler-core/dist/compiler-core",
      "@vue/compiler-dom/dist/compiler-dom",
      "@vue/compiler-sfc/dist/compiler-sfc",
      "@vue/compiler-ssr/dist/compiler-ssr",
      "@vue/reactivity/dist/reactivity",
      "@vue/runtime-core/dist/runtime-core",
      "@vue/runtime-dom/dist/runtime-dom",
      "@types/jest"
    ]
  },
  "include": [
    "src/*.ts",
  ],
  "exclude": [
    "node_modules",
    "dev",
    "tests",
    "src/types.ts"
  ]
}
