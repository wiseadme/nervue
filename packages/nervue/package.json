{
  "name": "nervue",
  "version": "0.0.8",
  "description": "Vue 3 composition and options API-compatible lightweight store library",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/types.d.ts",
  "scripts": {
    "dev": "webpack serve --hot --config build/webpack.dev.config.js",
    "build": "tsc -p ../../tsconfig.json && npm run test && webpack --config build/webpack.prod.config.js",
    "test": "jest",
    "lint": "eslint '**/*.ts' --ignore-pattern node_modules/",
    "lint:fix": "eslint '**/*.ts' --fix --ignore-pattern node_modules/",
    "lint:editorconfig": "editorconfig-checker"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wiseadme/nervue.git",
    "directory": "packages/nervue"
  },
  "bugs": {
    "url": "https://github.com/wiseadme/nervue/issues",
    "email": "wiseadme@gmail.com"
  },
  "keywords": [
    "nervue",
    "vue",
    "vue3",
    "vue3-composition-api",
    "store",
    "vue store",
    "composition-api",
    "state manager"
  ],
  "author": {
    "name": "Anar Allakhverdiev",
    "email": "wiseadme@gmail.com",
    "url": "https://projs.net"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^7.32.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^8.4.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.5.2",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-serializer-html": "^7.1.0",
    "regenerator-runtime": "^0.13.9",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/test-utils": "^2.0.0-rc.17",
    "vue": "^3.2.31",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "^17.0.0"
  },
  "peerDependencies": {
    "vue": "^3.2.31"
  }
}
